{
  "manifest_version": 2,
  "name": "Mnml Tab",
  "description": "A minimal new tab page",
  "version": "0.2",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage", "tabs", "downloads"],
  "icons": {
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "mnml-tab@atajsic.me"
    }
  }
}
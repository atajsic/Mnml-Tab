{
  "manifest_version": 3,
  "name": "Mnml Tab",
  "description": "A minimal new tab page",
  "version": "0.1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage", "tabs", "downloads"],
  "icons": {
    "48": "img/48.png",
    "128": "img/128.png"
  }
}